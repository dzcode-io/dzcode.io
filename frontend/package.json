{
  "name": "dzcode.io-frontend",
  "version": "0.1.0",
  "description": "frontend code for dzcode.io",
  "scripts": {
    "clear": "rimraf ./firebase/public",
    "dev": "yarn clear && cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js -wd --hot",
    "lint:check": "eslint . --ext .tsx,.ts,.js,.json --report-unused-disable-directives",
    "lint:fix": "eslint . --ext .tsx,.ts,.js,.json --quiet --fix",
    "test": "echo 0",
    "build": "yarn clear cross-env && NODE_ENV=production webpack --config ./webpack.config.js --display-error-details",
    "generate:sitemap": "node src/build/sitemap.js"
  },
  "repository": {
    "type": "git",
    "repository": "https://github.com/dzcode-io/dzcode.io.git"
  },
  "author": {
    "email": "contact@zakiii.com",
    "name": "Zakaria Mansouri",
    "url": "https://www.zakiii.com"
  },
  "license": "MIT",
  "private": "true",
  "engines": {
    "node": ">=10",
    "yarn": "1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
