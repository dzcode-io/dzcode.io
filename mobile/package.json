{
  "name": "@dzcode.io/mobile",
  "version": "1.1.2",
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "email": "omarbelghaouti@gmail.com",
    "name": "Omar Belghaouti",
    "url": "https://omarbelghaouti.space/"
  },
  "scripts": {
    "common:sync": "rimraf ./src/.common && syncdir ../common/src ./src/.common -c",
    "common:sync:watch": "yarn common:sync --watch",
    "expo": "expo",
    "dev": "expo start",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:check": "eslint \"**/*.*\" --report-unused-disable-directives",
    "eslint:fix": "eslint \"**/*.*\" --quiet --fix",
    "prettier:check": "prettier \"**/*.*\" --check --ignore-path ./.prettierignore",
    "prettier:fix": "prettier \"**/*.*\" --write --ignore-path ./.prettierignore",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:cov:watch": "jest --coverage --watchAll",
    "clean": "rimraf ./node_modules"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "lint-staged": {
    "*.*": [
      "eslint --quiet --fix --ignore-path .eslintignore",
      "prettier --write --ignore-path ./.prettierignore"
    ]
  },
  "private": true,
  "engines": {
    "node": ">=14",
    "yarn": ">=1.4.2"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.11.2",
    "@react-navigation/drawer": "^5.12.5",
    "@react-navigation/native": "~5.8.10",
    "@react-navigation/stack": "~5.12.8",
    "@types/jest": "^26.0.23",
    "expo": "~42.0.0",
    "expo-application": "~3.2.0",
    "expo-asset": "~8.3.2",
    "expo-constants": "~11.0.1",
    "expo-device": "~3.3.0",
    "expo-font": "~9.2.1",
    "expo-linking": "~2.3.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "expo-web-browser": "~9.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-web": "~0.13.12",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "sentry-expo": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "babel-plugin-transform-define": "^2.0.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-unicorn": "^24.0.0",
    "expo-cli": "^4.7.2",
    "internal-ip": "^6.2.0",
    "jest-expo": "^42.0.0",
    "prettier": "^2.3.2",
    "react-native-help-create": "^1.0.5",
    "react-test-renderer": "16.14.0",
    "typescript": "~4.0.0"
  }
}
